{
  "name": "front-end-spa-app-unsecured",
  "version": "1.0.0",
  "description": "Auth0 Labs Sample SPA Application",
  "license": "MIT",
  "author": "Auth0",
  "bugs": {
    "url": "https://github.com/auth0/auth0-product-education-labs/issues"
  },
  "homepage": "https://github.com/auth0/auth0-product-education-labs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/auth0-product-education-labs.git"
  },
  "scripts": {
    "start": "npx http-server . -p $npm_package_config_spa_app_port -c-1",
    "build": "parcel build app.js ./scripts/*.js --no-content-hash && find . -name '*.html' -not -path \"./node_modules/*\"| cpio -pdm ./dist"
  },
  "config": {
    "spa_app_port": 8000
  },
  "keywords": [
    "auth0",
    "vercel",
    "openid",
    "oidc",
    "auth",
    "authentication",
    "samples",
    "tools",
    "labs"
  ],
  "dependencies": {
    "parcel-bundler": "1.12.4",
    "envvar-allowlist": "auth0/parcel-plugin-envvar-allowlist"
  },
  "browserslist": [
    "last 1 Chrome version"
  ]
}
