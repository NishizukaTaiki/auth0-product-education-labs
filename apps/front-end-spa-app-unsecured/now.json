{
  "version": 2,
  "build": {
    "env": {
      "DOMAIN":"@DOMAIN",
      "CLIENT_ID":"@CLIENT_ID",
      "API_URL":"@API_URL"
    }
  },
  "builds": [
    {
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ]
}
